<%= form_with(model: student) do |form| %>
  <% if student.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(student.errors.count, "error") %> prohibited this student from being saved:</h2>

      <ul>
        <% student.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :roll_no %>
    <%= form.text_field :roll_no %>
  </div>

  <div class="field">
    <%= form.label :fathers_name %>
    <%= form.text_field :fathers_name %>
  </div>

  <div class="field">
    <%= form.label :CGPA %>
    <%= form.text_field :CGPA %>
  </div>

  <div class="field">
    <%= form.label :college_ID %>
    <%= form.number_field :college_ID %>
  </div>

  <div class="field">
    <%= form.label :last_semester %>
    <%= form.number_field :last_semester %>
  </div>

  <div class="field">
    <%= form.label :class_rank %>
    <%= form.number_field :class_rank %>
  </div>


  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
